local s_x = -1
local s_y = -1
local s_val = -1

function s_setx(x)
   s_x = x
end

function s_sety(y)
   s_y = y
end

function s_setval(val)
   s_val = val
end

function s_getx()
   return (s_x)
end

function s_gety()
   return (s_y)
end

function s_getval()
   return (s_val)
end